{
  "languageserver": {
     "haskell": {
      "command": "hie-wrapper",
      "rootPatterns": [
        ".stack.yaml",
        "cabal.config",
        "package.yaml"
      ],
      "filetypes": [
        "hs",
        "lhs",
        "haskell"
      ],
      "initializationOptions": {
        "languageServerHaskell": {
        }
      },
    },
    "ocaml": {
      "command": "ocaml-language-server",
      "args": [
        "--stdio"
      ],
      "filetypes": [
        "ocaml"
      ]
    },
    "reason": {
      "command": "reason-language-server",
      "filetypes": ["reason"]
    }
  },
  "diagnostic.errorSign": "âœ˜",
  "codeLens.enable": true,
}
