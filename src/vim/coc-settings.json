{
  "languageserver": {
     "haskell": {
      "command": "hie-wrapper",
      "rootPatterns": [
        ".stack.yaml",
        "cabal.config",
        "package.yaml"
      ],
      "filetypes": [
        "hs",
        "lhs",
        "haskell"
      ],
      "initializationOptions": {
        "languageServerHaskell": {
        }
      },
    },
    "ocaml-lsp": {
      "command": "opam",
      "args": [
        "config", "exec", "--", "ocamllsp"
      ],
      "filetypes": [
        "ocaml", "reason"
      ]
    }
  },
  "diagnostic.errorSign": "âœ˜",
  "codeLens.enable": true,
}
